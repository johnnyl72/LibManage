<div class="jumbotron jumbotron-fluid">
    <div class="container">
        <h1 class="display-4">Account Status</h1>
    </div>
</div>
<div>
    <h1>Your loans!</h1>
    <% current_user.loans.each do |loan| %>
        <div>
            <%= content_tag :h3, "Loan for book: #{loan.book_item.book.title}" %>
            <%= content_tag :p, "Due: #{loan.due_date}" %>
        </div>
    <% end %>
</div>
